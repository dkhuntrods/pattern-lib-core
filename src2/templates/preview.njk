<!doctype html>
<html class="no-js" lang="">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <title>{{page.title}} {{site.title}}</title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">
    </head>
    <body>

        <h1>{{page.title}}</h1>
        <ul>
        {% for blockId in site.blockIds %}
          <li>
            <p>{{ blockId }}</p>
            <div>{{ blockId | renderXSL | safe}}</div>
          </li>

        {% else %}
          <li>This would display if the 'site.blockIds' collection were empty</li>
        {% endfor %}
        </ul>


    </body>
</html>
