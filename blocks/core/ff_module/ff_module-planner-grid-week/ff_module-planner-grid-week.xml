<planner date="2015-11-11" today-url="planner" previous-day-url="planner/day/2015-11-10" next-day-url="planner/day/2015-11-12">

    <timeranges>
        {% for time in timeranges %}
        <time start="{{time.start}}" end="{{time.end}}" />
        {% endfor%}
    </timeranges>
    <events>

    {% for event in events %}
    <event subject="{{event.subject}}" isostartdate="{{event.isostartdate}}" isoenddate="{{event.isoenddate}}" description="{{event.description}}" url="{{event.url}}" colour="{{event.colour}}" location="{{event.location}}" minutelength="60" minutestart="240" >

        {% if event.note %}
        <note>
        <![CDATA[
        {{ event.note | safe }}
        ]]>
        </note>
        {% else %}
        <note/>
        {% endif %}

    </event>
    {% endfor %}
</events>
</planner>
