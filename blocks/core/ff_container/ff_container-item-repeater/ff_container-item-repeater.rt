<div class="ff_container-item-repeater">
    <ol class="ff_container-item-repeater__items">
        <li class="ff_container-item-repeater__item{this.props.modifier ? ' ff_container-item-repeater__item--'+this.props.modifier : ''}"
            rt-repeat="child in React.Children.toArray(this.props.children)" key="{child.key}">{child}</li>
    </ol>
</div>
